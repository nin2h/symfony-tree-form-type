{
    "name": "mrself/symfony-tree-form-type",
    "version": "1.7.7",
    "type": "project",
    "license": "MIT",
    "description": "Symfony package for tree form type",
    "require": {
        "php": ">=7.2.0",
        "symfony/form": ">=4.3.11",
        "symfony/framework-bundle": ">=4.3.11",
        "symfony/yaml": "^5.2"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/orm": "^2.7",
        "phpunit/phpunit": "^7",
        "symfony/doctrine-bridge": "^4",
        "symfony/http-kernel": "^4",
        "symfony/security": "^4",
        "symfony/twig-bridge": "^4",
        "symfony/twig-bundle": "^4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Mrself\\TreeTypeBundle\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mrself\\TreeTypeBundle\\Tests\\": "./tests/"
        }
    }
}
